@model Chat

<div class="chat-body">
    @foreach (var message in Model.Messages)
    {
        <div class="message">
            <header>@message.Name:</header>
            <p>@message.Text</p>
            <footer>@message.TimeStamp</footer>
        </div>
    }
</div>

<div class="chat-input">
    <input type="text">
    <button>Send</button>
</div>